{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Multi-Vendor Marketplace Application",
    "keywords": ["laravel", "framework", "marketplace", "multi-vendor"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "akaunting/laravel-money": "v6.0.2",
        "laravel/framework": "v12.21.0",
        "laravel/jetstream": "v5.3.8",
        "laravel/sanctum": "v4.2.0",
        "laravel/tinker": "v2.10.1",
        "livewire/livewire": "v3.6.4",
        "pragmarx/google2fa-laravel": "v2.3.0",
        "spatie/laravel-activitylog": "v4.10.2",
        "spatie/laravel-medialibrary": "v11.13.0",
        "spatie/laravel-permission": "v6.21.0",
        "spatie/laravel-settings": "v3.4.4",
        "spatie/laravel-sluggable": "*",
        "spatie/laravel-translatable": "v6.11.4"
    },
    "require-dev": {
        "fakerphp/faker": "v1.24.1",
        "laravel/pint": "v1.24.0",
        "laravel/sail": "v1.44.0",
        "mockery/mockery": "v1.6.12",
        "nunomaduro/collision": "v8.8.2",
        "phpunit/phpunit": "v12.3.0",
        "spatie/laravel-ignition": "v2.9.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Modules\\": "app/Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
